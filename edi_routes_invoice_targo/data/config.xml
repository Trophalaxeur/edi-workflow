<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="edi_invoice_targo_out" model="edi.tools.edi.flow">
            <field name="name">Invoice TARGO(out)</field>
            <field name="direction">out</field>
            <field name="model">account.invoice</field>
            <field name="method">send_edi_export_invoice_targo</field>
            <field name="partner_resolver"></field>
        </record>
    </data>
    <data>
        <record id="view_res_partner_form" model="ir.ui.view">
            <field name="name">view.res.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <field name="email" position="after">
                    <field name="targo_reference"/>
                </field>
            </field>
        </record>
    </data>
</openerp>
